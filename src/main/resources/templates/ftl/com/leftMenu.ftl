            <script type="text/javascript">
                $(document).ready(function () {
                    try {
                        var _curr = 'http://member.aysmzj.gov.cn/';
                        if (window.location.href == _curr) {
                            $.cookie(_curr, null, {path: '/', domain: 'aysmzj.gov.cn'});
                        }
                        $('.menus .menu').click(function () {
                            $('.menus .submenus').slideUp(300);
                            $(this).next().slideDown(300);
                        });
                        $('.menus .submenus a').click(function () {
                            var _value = $(this).attr('href');
                            $.cookie(_curr, _value, {path: '/', domain: 'aysmzj.gov.cn'});
                        });
                        if ($.cookie(_curr) != null && $.cookie(_curr) != undefined && $.cookie(_curr) != '') {
                            var _submenus = $('.menus .submenus a[href="' + $.cookie(_curr) + '"]').parents('.submenus');
                            _submenus.show();
                            $('.menus .submenus a[href="' + $.cookie(_curr) + '"]').css('color', '#f50');
                        } else {
                            $('.menus .submenus:first').show();
                        }
                    } catch (e) {
